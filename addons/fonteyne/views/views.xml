<?xml version="1.0"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="view_pos_fidelity_form">
            <field name="name">pos.fidelity.form.view</field>
            <field name="model">pos.fidelity</field>
            <field name="inherit_id" ref="point_of_sale.view_pos_fidelity_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='rounding']" position="after">
                    <field name="minimum_sale" />
                    <field name="discount_product_id" />
                </xpath>
            </field>

        </record>

    </data>
</openerp>
