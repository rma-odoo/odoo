<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="project_tt_rated" model="project.task.type">
            <field name="sequence">25</field>
            <field name="name">Rated</field>
            <field name="case_default" eval="True"/>
            <field name="fold" eval="True"/>
        </record>

        <record id="task_rating_request" model="email.template">
            <field name="name">Task Rating Request</field>
            <field name="email_from">${object.user_id.email or ''}</field>
            <field name="subject">${object.name}</field>
            <field name="model_id" ref="rating_project.model_project_task"/>
            <field name="email_to" >${object.partner_id.email}</field>
            <field name="partner_to">${object.partner_id.id}</field>
            <field name="auto_delete" eval="True"/>
            <field name="body_html"><![CDATA[ 
<body>
    <div style="height:auto; margin-left:30px; margin-top:10px;">
        <br><h1 style="margin-top:15px; margin-bottom:20px;color:#A9AEE1;" align="center">${object.name or ''}</h1>
    </div>
    <div style="max-width:600px; height:auto; margin-left:30px;">
        <br><h4 style="margin-left:50px;font-weight:normal;">Information about the ${object.name or ''} </h4>
        <h4 style="margin-left:50px;font-weight:normal;">Project : ${object.project_id.name or ''} </h4>
        <h4 style="margin-left:50px;font-weight:normal;">Assigned To : ${object.user_id.name or ''} </h4>
        <h4 style="margin-left:50px;font-weight:normal;">Description : ${object.description or ''} </h4>
        <h4 style="margin-left:50px;">How was the ${object.name or ''} provided?</h4>
        <div style="max-width:600px;height:150px;align=center;" >
            <div style="width:170px;align:center;float:left;">
                <a href="/rating/project.task/${object.id}/great">
                    <img style="margin-left:60px; margin-top:10px; " alt="Great"  src="data:image/png;base64,"  />
                </a>
                <div style="margin-left:40px;margin-top:20px;font-size:19px;font-weight:bold;text-decoration:underline;color:green;">It was great</div>
            </div>
            <div style="width:170px;align:center;float:left;">
                <a href="/rating/project.task/${object.id}/okay">
                    <img style="margin-left:70px; margin-top:10px;" alt="Idle" src="data:image/png;base64," />
                </a>

            <div style="margin-left:68px;margin-top:20px;font-size:19px;font-weight:bold;text-decoration:underline;color:orange;">It was ok</div>
            </div>
            <div style="width:230px;align:center;float:left;">  
                <a href="/rating/project.task/${object.id}/bad">
                    <img style="margin-left:95px; margin-top:10px;" alt="Bad" src="data:image/png;base64," />
                </a>
            <div style="margin-left:70px;margin-top:20px;font-size:19px;font-weight:bold;text-decoration:underline;color:red;">It wasn't good</div>	
            </div>
        </div>
    </div>
</body>
                ]]>
            </field>
        </record>
    </data>
</openerp>
